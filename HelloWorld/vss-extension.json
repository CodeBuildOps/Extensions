{
    "manifestVersion": 1,
    "id": "First-Extension-HelloWorld",
    "version": "1.0.0",
    "name": "Hello World",
    "publisher": "Helloworld-task",
    "description": "A Azure Build Task that prints Hello World to the Console.",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "icons": {
        "default": "images/Hello.png"
    },
    "categories": [
        "Azure Pipelines"
    ],
    "contributions": [
        {
            "id": "Helloworld",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "buildAndReleaseTask"
            }
        }
    ],
    "files": [
        {
            "path": "buildAndReleaseTask"
        }
    ]
}