  "name": "HelloWorld",
  "friendlyName": "Hello World",
  "description": "Prints Hello World to the console",
  "author": "Abhishek Kumar Singh",
  "helpMarkDown": "This task prints a message to the console.",
  "category": "Utility",
  "visibility": [
    "Build",
    "Release"
  ],
  "runsOn": [
    "Agent",
    "DeploymentGroup"
  ],
  "version": {
    "Major": "1",
    "Minor": "0",
    "Patch": "0"
  },
  "instanceNameFormat": "Prints Hello $(name)",
